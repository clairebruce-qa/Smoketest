public class TestOptionFiles extends TeraBoxTestCase {

	//Clase adaptada a R2	
	@Test
	public void testTB604() throws Exception  {
		
		//TB-604:Move_File:Mover
		
		Tb.login(selenium, username, password);
		
		//clear OnlineFiles
		Tb.deleteAllOnlineFiles(selenium);
		
		//create a folder
		Assert.assertEquals("ok", Tb.createFolder(selenium, "test1"));
		
		//Upload a file
		Tb.uploadFile(selenium,"dns.txt");
		
		Tb.onlineFilesMoveFile(selenium, "dns.txt", "test1");
			
		Tb.logout(selenium);
		
	}

